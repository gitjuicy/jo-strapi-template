{
  "collectionName": "components_ui_links",
  "info": {
    "displayName": "Link",
    "icon": "link"
  },
  "options": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "page": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::page.page",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "external"
            },
            false
          ]
        }
      }
    },
    "url": {
      "type": "string",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "external"
            },
            true
          ]
        }
      }
    },
    "external": {
      "type": "boolean",
      "default": false
    }
  },
  "config": {}
}
